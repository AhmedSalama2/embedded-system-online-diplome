#include <stdio.h>
#include <math.h>
double squareroot(float x){
	int high=x;
	int low =0;
	int temp;
	int i;
	for (i=0;i<100;++i){
		temp=(high+low)/2;
		if (temp*temp==x){
			return temp;
		}
		else if (temp*temp==x){
			high=temp;
		}
		else{
			low =temp;
		}
	}
	return temp;
}
int main (){
	int x;
	printf("please enter the number ");
	fflush(stdin);fflush(stdout);
	scanf("%d",&x);
	printf ("sqroot of %d=%f",x,squareroot(x));
	return 0;
}

