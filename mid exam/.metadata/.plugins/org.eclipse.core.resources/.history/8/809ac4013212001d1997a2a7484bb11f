#include<stdio.h>
int main (){
	int postion=0;
	int swap;
	int x[10]={0,-10,2,6,9,6,1,0,-10,2};
	int i;
	int j;
	for (i=0;i<10;++i){
		postion=i;
		for ( j=i;j<10;++j){

			if (x[postion]>x[j]){

				postion=j;
			}
			if (postion!=i){
				swap=x[i];
				x[i]=x[postion];
				x[postion]=swap;}
		}
	}
	for( i=0;i<10;++i){
		if (i>0){
			if (x[i]!=x[i-1]&&x[i]!=x[i+1]){
				printf ("%d",x[i]);
			}
			else{if (x[i]!=x[i+1]){
				printf ("%d",x[i]);
			}
			}
		}

	}

	return 0;
}

